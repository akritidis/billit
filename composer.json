{
  "name": "drakakisgeo/billit",
  "description": "The php client for Billit.io API",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "George Drakakis",
      "email": "george@drakakis.gr"
    }
  ],
  "require": {
    "guzzlehttp/guzzle": "~7.0"
  },
  "autoload": {
    "psr-4": {
      "Drakakisgeo\\Billit\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Drakakisgeo\\Billit\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "mockery/mockery": "^1.4",
    "orchestra/testbench": "^6.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Drakakisgeo\\Billit\\BillitServiceProvider"
      ],
      "alias": {
        "Billit": "Drakakisgeo\\Billit\\Facade"
      }
    }
  }
}
